        <rdfs:label xml:lang="en">Field object</rdfs:label>
        <rdfs:label xml:lang="en">FieldTheory</rdfs:label>
        <rdfs:label xml:lang="ru">Элемент теории полей</rdfs:label>
    </owl:Class>
    


