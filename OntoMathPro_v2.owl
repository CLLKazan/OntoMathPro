    <owl:Class rdf:about="http://ontomathpro.org/omp2#DifferentialEquation%2CPartial%2CMethods">
        <rdfs:subClassOf rdf:resource="http://ontomathpro.org/omp2#E464"/>
        <rdfs:label xml:lang="en">Differential equation, partial, methods</rdfs:label>
        <rdfs:label xml:lang="ru">Дифференциальное уравнение с частными производными, методы</rdfs:label>
